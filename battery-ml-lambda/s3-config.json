{
  "cors_configuration": {
    "CORSRules": [
      {
        "AllowedHeaders": ["*"],
        "AllowedMethods": ["GET", "HEAD", "PUT", "POST"],
        "AllowedOrigins": [
          "http://localhost:5173",
          "http://localhost:3000",
          "https://zeflash.com",
          "https://*.zeflash.com"
        ],
        "ExposeHeaders": ["ETag", "Content-Type"],
        "MaxAgeSeconds": 3600
      }
    ]
  },
  "bucket_policy": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "PublicReadGetObject",
        "Effect": "Allow",
        "Principal": "*",
        "Action": "s3:GetObject",
        "Resource": "arn:aws:s3:::battery-ml-results-070872471952/battery-reports/*"
      }
    ]
  }
}
